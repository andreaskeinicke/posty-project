# Server Configuration
PORT=3001
NODE_ENV=development

# Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Domain APIs (choose one or multiple)
# GoDaddy API (for domain availability)
GODADDY_API_KEY=your_godaddy_key_here
GODADDY_API_SECRET=your_godaddy_secret_here

# Alternative: Namecheap API
NAMECHEAP_API_USER=your_username_here
NAMECHEAP_API_KEY=your_namecheap_key_here

# Alternative: Domain.com API
DOMAIN_API_KEY=your_domain_api_key_here

# CORS Settings
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
